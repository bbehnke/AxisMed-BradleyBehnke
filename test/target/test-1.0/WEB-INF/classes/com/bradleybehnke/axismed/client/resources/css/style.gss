
@def C_PRIMARY #505DA7;
@def HEADERBACKGROUND resourceUrl("headerBackground");
@def BACKGROUND resourceUrl("background");

body {
    overflow: hidden;
    background: BACKGROUND;
    background-size: 100vw 100vh;
}

td{
  vertical-align: top;
  text-align: left;
}

h3 {
    margin: 5px;
}

p {
    margin-top: -2px;
    padding-left:5px;
    padding-right:5px;
    font-family: Arial;
    font-size: 12px;
}

a {
    text-decoration:none;
}

.lblName {
}

.txtName {
}

.lblStarted {
}

.datePicker {
    width:50px;
}

.btnReset {
    display: block;
    margin: auto;
}

.hobby2Outer {
    margin:15px;
}

.hobby2Outer h3 {
    color:white;
}

.hobby2Middle {
    background: C_PRIMARY;
    width: 350px;
    height: 100%;
    border-radius:5px;
    margin-top: 10px;
    padding:10px;
}

.hobby2Inner {
    background:white;
    height:100%;
    padding:10px;
}

.hobby2Inner td {
    padding:10px;
}

.hobby2Inner table {
    margin: 0 auto;
}

.hobby2Inner input {
    width: 200px;
}

.hobbyTable {
    width: 768px;
    margin-left: auto;
    margin-right: auto;
}

.hobbyTable p {
    margin-top: 0px;
    padding-left:5px;
    padding-right:5px;
    font-family: Arial;
    font-size: 12px;
}

.hobbyTable a {
    background: transparent;
    COLOR: black;
    margin: 5px;
    font-weight:bold;
    text-decoration: none;
}

.hobbyMiddleSmall {
    background: C_PRIMARY;
    width: 360px;
    height: 205px;
    border-radius:5px;
    margin-top: 10px;
    margin-right: 10px;
    padding:10px;
}

.hobbyMiddleEndSmall {
    background: C_PRIMARY;
    width: 360px;
    height: 205px;
    border-radius:5px;
    margin-top: 10px;
    padding:10px;
}

.hobbyInnerSmall {
    background: white;
    height: 203px;
}

.hobbyImageSmall{
    float:left;
}

.hobby1Image{
    float:left;
}


.hobbyOuter {
    position: relative;
    width: 100%;
    height: calc(100vh - 90px);
}

.hobbyMiddle {
    background: C_PRIMARY;
    width: 520px;
    height: 290px;
    position: absolute;
    top:0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    border-radius: 5px;
}

.hobbyInner {
    background: white;
    margin: 0 auto;
    width: 500px;
    height: 270px;
    position: relative;
    top: 50%;
    -webkit-transform: translateY(-50%)
}

.header {
	width: 100%;
	height: 40px;
	margin: 0 auto;
	background: HEADERBACKGROUND;
	background-size: 100vw 100%;
	padding: 25px;
}

.name {
	margin-left: 12%;
	font-weight: bold;
	font-size: xx-large;
	color: white;
	font-family: Georgia, serif;
}

.btnHome {
    float: right;
    margin-right: 300px;
}

.btnHobby1 {
    float: right;
    margin-right: -185px;
}


.btnHobby2 {
    float: right;
    margin-right: -320px;
}

button {
	background: blue;
	background-image: -webkit-linear-gradient(top, #3498db, #2980b9);
	-webkit-border-radius: 28;
	-moz-border-radius: 28;
	border-radius: 28px;
	border-width:0px;
	font-family: Arial;
	color: #ffffff;
	font-size: 12px;
	padding: 10px 20px 10px 20px;
	text-decoration: none;
}

button:hover {
	background: #3cb0fd;
	background-image: -webkit-linear-gradient(top, #3cb0fd, #3498db);
	text-decoration: none;
}